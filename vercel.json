{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/user(.*)",
            "dest": "/route/userRouter.js"
        },
        {
            "src": "/vendor(.*)",
            "dest": "/route/vendorRouter.js"
       },
        {
            "src": "/(.*)",
            "dest": "/app.js",
            "headers": {
                "Authorization": "null"
            }
        }
    ]
}
